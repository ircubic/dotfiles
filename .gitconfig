[user]
	email = ircubic@gmail.com
	name = Daniel Bruce
[core]
	excludesfile = /home/cubic/.gitignore
[color]
	ui = auto
[pack]
	threads = 2
[alias]
	st = status
	ci = commit
	br = branch
	brn = checkout -b
	co = checkout
	df = diff
	wdiff = diff --color-words
	sq = merge --squash

	svm = merge --no-ff --no-commit
    svr = svn rebase

    iad = add --interactive
	ici = commit --interactive
    aci = commit --amend

    rb = rebase
    rbi = rebase -i
    rbc = rebase --continue
    rbs = rebase --skip

    vgc = repack -f -a -d --depth=250 --window=250
    nvgc = !ionice -n7 nice -n20 git vgc
[merge]
	tool = emerge
[branch]
	autosetuprebase = always
[help]
	autocorrect = 1
